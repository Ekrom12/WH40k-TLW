    "assetCatalog": {
      "Adaptive Physiology§Adrenal Glands": {
        "keywords": {
          "Keywords": [
            "Adaptive Physiology"
          ]
        },
        "text": "A unit with this Adaptation gains theSprint (1) special rule. Units that already have the Sprint (x) special rule increase the value of (x) by 1"
      },
      "Adaptive Physiology§Caustic Blood": {
        "keywords": {
          "Keywords": [
            "Adaptive Physiology"
          ]
        },
        "text": "When a unit with this wargear is\nslain and the last model in the unit\nis removed from play, before that\nmodel is removed, all enemy units\nwithin 6” take D3+(x) automatic\nhits where (x) is the models\nWounds (W) characteristic. The\nStrength of the hit is equal to the\nremoved units Toughness (T)\ncharacteristic. The AP is resolved\nat AP5."
      },
      "Adaptive Physiology§Toxin Sacs": {
        "keywords": {
          "Keywords": [
            "Adaptive Physiology"
          ]
        },
        "text": "All units with this Adaptation gain the\nPoison (5+) special rule on all\nweapons they are equipped with. If a\nmodel with this Adaptation already has\nthe Poison (x) special rule, increase\nthe value of (x) by 1 for that unit."
      },
      "Bio-Morph§Dessicator Larvae": {
        "text": "Fleshborers or Fleshborer Hives of the equipped unit gain the **Monster Hunter** Special rule."
      },
      "Bio-Morph§Electro Shock Grubs": {
        "text": "Fleshborer’s of the equipped unit gain the **Haywire (6+)** special rule. \nFleshborer Hives gain the **Haywire (5+)** special rule."
      },
      "Bio-Morph§Scorch Bugs": {
        "text": "Fleshborer’s of the equipped unit gain the **Rending (6+)** special rule. \nFleshborer Hives gain the **Rending (5+)** special rule."
      },
      "Bio-Morph§Wings": {
        "text": "A Tyranid Infantry or Monster unit with this Bio-Morph\nincreases their Movement (M) characteristic to 12”. It\ngains the Hammer of Wrath (1) and Deep strike special\nrules. The equipped model gains the Fly keyword."
      },

      "Wargear§Artificer Armour": {
        "inheritance": "removed"
      },
      "Wargear§Atomantic Shielding": {
        "text": "This confers a 5+ Invulnerable save. When a model with this wargear is slain and removed from\nplay, the model suffers an Explodes result per the Vehicle damage chart before it is removed from play."
      },
      "Wargear§Aura of Dark Glory": {
        "text": "A model with a Aura of Dark Glory has a 5+ Invulnerable save."
      },
      "Wargear§Auspex Scanner": {
        "text": "Enemy units may not Infiltrate within\n18” of a model equipped with this\nwargear."
      },
      "Wargear§Bionics": {
        "text": "A model with this wargear has a 6+\nInvulnerable save."
      },
      "Wargear§Cataphractii Terminator Armour": {
        "text": "An ancient heavier pattern of Terminator Armour rarely\nused. It provides a 2+ Armour save and a 4+ Invulnerable\nsave. The Wearer gains the Relentless, Bulky (2), Deep\nStrike, Relentless and Slow & Purposeful special rules.\nThe wearers Movement (M) Characteristic is decreased\nby 1”"
      },
      "Wargear§Chaos Familiar": {
        "text": "A model with Chaos Familiar gains\nthe Furious Charge (1) special rule."
      },
      "Wargear§Chaos Runes": {
        "text": "A model with a Chaos Runes has a\n6+ Invulnerable save."
      },
      "Wargear§Chaos Sigil": {
        "text": "A model with a Chaos Sigil has a 4+\nInvulnerable save."
      },
      "Wargear§Daemon Engine Armor": {
        "text": "The living armoured skin of the Dameon engines crafted by Chaos artifice. It confers a 3+ armour save."
      },
      "Wargear§Daemonic Steed": {
        "text": "A unit that is equipped with a Daemonic Steed becomes\na Cavalry type unit. In addition, it gains +1 Strength (S),\nToughness (T) and Wounds (W) Characteristics."
      },
      "Wargear§Dreadnought Plate": {
        "text": "These ancient and powerful mechanised war suit\nencompasses a sarcophagus where an ancient warrior\nlay entombed. When combined, the warrior may take to\nthe field again with a new body. It confers a 2+ Armour\nsave."
      },
      "Wargear§Explorator Augury Web": {
        "text": "The Transport capacity of this unit is reduced to 6 Infantry models. This unit gains the Scout special\nrule."
      },
      "Wargear§Icon of Despair": {
        "text": "A unit that includes a model with an\nIcon of Despair when charged\nalways count any charges\nperformed against it as Disordered\nCharges."
      },
      "Wargear§Icon of Excess": {
        "text": "A unit that includes a model with an\nIcon of Excess gains the Fleet\nspecial rule."
      },
      "Wargear§Icon of Flame": {
        "text": "A unit that includes a model with an Icon of Flame gain the Arcing (6+) special rule on all melee weapons they are\nequipped with."
      },
      "Wargear§Icon of Wrath": {
        "text": "A unit that includes a model with an\nIcon of Wrath can re-roll Charge\nrolls."
      },
      "Wargear§Indomitus Terminator Armour": {
        "text": "The Standard Terminator Armour used by most Astartes\nforces. It provides a 2+ Armour Save and a 5+\nInvulnerable Save. The wearer gains the Relentless,\nBulky (2) and Deep Strike special rule. Models equipped\nwith this Armour may not perform Sweeping Advances"
      },
      "Wargear§Mark of Chaos Undivided": {
        "keywords": {
          "Keywords": [
            "Chaos Marks"
          ]
        },
        "text": "A unit with this special rule gains the Zealot (1) special rule and the Undivided Keyword. If the unit already has the\nZealot (x) special rule, the unit increases the value of (x) by 1"
      },
      "Wargear§Mark of Khorne": {
        "keywords": {
          "Keywords": [
            "Chaos Marks"
          ]
        },
        "text": "A unit with this Mark gains the Hatred special rule and\nthe Khorne Keyword"
      },
      "Wargear§Mark of Nurgle": {
        "keywords": {
          "Keywords": [
            "Chaos Marks"
          ]
        },
        "text": "A unit with this Mark gains the Feel no Pain (5+) special\nrule and the Nurgle Keyword. Vehicle and Aircraft\nunits with this Mark gain the benefit of Light cover even\nwhen they are in open terrain, instead of the Feel No\nPain (x) special rule."
      },
      "Wargear§Mark of Slaanesh": {
        "keywords": {
          "Keywords": [
            "Chaos Marks"
          ]
        },
        "text": "A unit with this Mark gains +1 to their Initiative (I)\ncharacteristic and the Slaanesh keyword. Vehicle and\nAircraft units with this special rule gain +2” to their\nMovement (M) characteristic instead."
      },
      "Wargear§Mark of Tzeentch": {
        "text": "A unit with this Mark gains +1 to their Invulnerable\nsaves to a max of 3+ and the Tzeentch Keyword. If the\nunit does not have an Invulnerable save, it gains a 6+\nInvulnerable save."
      },
      "Wargear§Narthecium": {
        "text": "A model with this Wargear and their attached unit gain Feel No Pain (5+) Special rule."
      },
